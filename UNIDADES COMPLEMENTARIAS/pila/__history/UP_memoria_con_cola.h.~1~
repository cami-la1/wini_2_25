#ifndef pila2H
#define pila2H
#include "UC_memoria.h"
//---------------------------------------------------------------------------

class pilaM {
private:
    colaM* c; // Único atributo, una referencia a una cola

public:
    // Constructor que recibe una cola
    pilaM(colaM* cola);

    // Métodos de la pila
    bool Vacia();
    void Meter(int e);
    void Sacar(int &e);
    int cima();
    void imprimir(TColor FormColor, TCanvas *Canvas);
};

#endif

