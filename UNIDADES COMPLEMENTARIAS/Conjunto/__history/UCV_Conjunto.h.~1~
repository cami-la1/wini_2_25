class conjunto{
	private:


    static const int MAX = 100;
	static const int NULO = -1;
	typedef int Direccion;

	int elementos[MAX];
	int longitud;
	 public   :
     void crear();
	 bool vacio();
	 bool pertenece(int e);
	 void inserta (int e );
	 void suprime(int e);
	 int cardinal();
	 int ordinal(int e)   ;
     int muestrea();
};

#endif
